
public class CarDefinition extends Vehicle{
  
	  
	  private int speed;
	  
	 
	  
	  
	  
	  public CarDefinition()
	  { 
		  super();
		  
		  speed=0;
		 
		
	  }
	  
	  public CarDefinition(String make,int speed)
	  { 
		  super();
		  this.speed=speed;
		  
		 
	  }

	public int getSpeed() {
		return speed;
	}

	public void setSpeed(int speed) {
		this.speed = speed;
	}
	 public String StartIt()
	 { 
		 return "Started!";
	 }
	public String Acceleration(int speed)
	{   
		String status="";
		
		if(speed==0)		{ 
			status="stopped";
		}
		if(speed==1)
		{   
			
			status= "accelerating";
		}
		return status;
	}


	
}
