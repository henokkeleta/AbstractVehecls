
public class VehicleRunnerc {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car myCar = new Car("Volvo",50,5000);
		myCar.setSpeed(50);
		Plane myPlane=new Plane("Boeing",450,1000);
		myPlane.setSpeed(450);
		Boat myBoat =new Boat("dream",60,"Atlantic");
		myBoat.setSpeed(60);
		myBoat.setLocation("Atlantic");
		System.out.println(myCar.vehicleStatus());
		System.out.println(myBoat.vehicleStatus());
		System.out.println(myPlane.vehicleStatus());

	}

}
