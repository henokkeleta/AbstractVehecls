
public class VehicleRunnerc {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car myCar = new Car();
		myCar.setSpeed(50);
		Plane myPlane=new Plane();
		myPlane.setSpeed(450);
		Boat myBoat =new Boat();
		myBoat.setSpeed(60);
		myBoat.setLocation("Atlantic");
		System.out.println(myCar.vehicleStatus());
		System.out.println(myBoat.vehicleStatus());
		System.out.println(myPlane.vehicleStatus());

	}

}
